<policies>
	<inbound>
		<base />
		<set-backend-service base-url="https://api.openweathermap.org/data/2.5" />
		<set-query-parameter name="appid" exists-action="override">
			<value>Put your API key here</value>
		</set-query-parameter>
	</inbound>
	<backend>
		<base />
	</backend>
	<outbound>
		<base />
	</outbound>
	<on-error>
		<base />
	</on-error>
</policies>